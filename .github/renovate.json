{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:best-practices",
    "default:disableRateLimiting"
  ],
  "baseBranches": [
    "main",
    "main-2"
  ],
  "rebaseWhen": "conflicted",
  "rebaseLabel": "rebase",
  "automerge": true,
  "automergeStrategy": "rebase",
  "automergeType": "pr",
  "autoApprove": true,
  "packageRules": [],
  "labels": [
    "automerge",
    "dependencies"
  ]
}
